{
    "name": "fesor/request-objects",
    "description": "Provides way of mapping http requests to objects",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Protko",
            "email": "fesors@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fesor\\RequestObject\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.0",
        "symfony/framework-bundle": "^3.0",
        "symfony/validator": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "friendsofphp/php-cs-fixer": "^1.1"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --testsuite unit",
            "vendor/bin/phpunit --testsuite functional"
        ]
    }
}
